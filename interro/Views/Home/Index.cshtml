@{
    ViewData["Title"] = "Prochaine fermeture";
}

<div class="text-center">
    <h1 class="display-4">Prochaine fermeture</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
<tbody>
    @foreach (var bridge in Model)
    {
        <tr>
            <td>@bridge.boat_name</td>
            <td>@bridge.closing_type</td>
            <td>@bridge.closing_date</td>
            <td>@bridge.reopening_date</td>
        </tr>
    }
</tbody>